{
    "manifest_version": 2,
    "name": "Scientific To Decimal Converter",
    "description": "Convert scientific notation to decimal notation at anytime while you browsing",
    "version": "1.0",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "ui.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/mani.js"]
    }],
    "permissions": [
        "activeTab"
    ]
}